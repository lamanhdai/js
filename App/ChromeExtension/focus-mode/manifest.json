{
  "manifest_version": 3,
  "description": "Toggle badge on chome extension",
  "version": "2.0",
  "action": {
    "default_icon": {
      "16": "favicon.ico",
      "32": "favicon.ico",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  },
  "short_name": "React App",
  "name": "auto populate form",
  "icons": {
    "16": "favicon.ico",
    "32": "favicon.ico",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U"
      }
    }
  }
}
